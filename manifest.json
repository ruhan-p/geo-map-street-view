{
  "manifest_version": 3,
  "name": "Geoguessr Map Street View",
  "version": "1.0",
  "description": "Shows the Google Map Street View Layer on the site",
  "content_scripts": [
    {
      "matches": ["https://www.geoguessr.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["injector.js"],
      "matches": ["<all_urls>"]
    }
  ]
}